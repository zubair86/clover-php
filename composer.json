{
  "name": "guesl/clover-php",
  "description": "Clover APIs PHP Helper Library SDK wrote by Jianfeng Li, Guesl.",
  "keywords": [
    "Clover",
    "laravel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/guesl/clover-php",
  "support": {
    "issues": "https://github.com/guesl/clover-php/issues",
    "source": "https://github.com/guesl/clover-php"
  },
  "authors": [
    {
      "name": "Jianfeng Li",
      "email": "modifyljf@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "^6.2",
    "phpseclib/phpseclib": "^2.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0|~5.0",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "psr-4": {
      "Guesl\\Clover\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guesl\\Clover\\Providers\\CloverServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
